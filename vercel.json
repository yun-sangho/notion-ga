{
  "version": 2,
  "name": "notion-ga",
  "alias": ["notion-ga"],
  "builds": [{ "src": "**/*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      },
      "src": "/collect",
      "dest": "/src/collect.js"
    }
  ]
}
